;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	   p2pcc. root.p2pcc. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.p2pcc.
;
dns     IN      A       10.4.4.1
;
Portatil1       IN       A      10.1.1.1
Portatil2       IN       A      10.1.1.2
PC1             IN       A      10.2.2.1
PC2             IN       A      10.2.2.2
Roma            IN       A      10.3.3.1
Paris           IN       A      10.3.3.2
